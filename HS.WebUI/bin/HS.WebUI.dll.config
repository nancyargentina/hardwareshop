<?xml version="1.0" encoding="utf-8"?>

<!--
  Para obtener más información sobre cómo configurar la aplicación de ASP.NET, visite
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
	<connectionStrings>
		<!-- Cadena de conexión al origen de datos-->
		<add name="Default"
			  connectionString="Data Source=.;Initial Catalog=HS;Integrated Security=True" providerName="System.Data.SqlClient" />
	</connectionStrings>

	<system.web>
		<compilation debug="true" strict="false" explicit="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
		<authentication mode="Forms">
			<forms loginUrl="~/Login.aspx" timeout="2880" />
		</authentication>

		<membership>
			<providers>
				<clear/>
			</providers>
		</membership>

		<profile>
			<providers>
				<clear/>
			</providers>
		</profile>

		<roleManager enabled="false">
			<providers>
				<clear/>
			</providers>
		</roleManager>

		<!-- especificacion de la cultura predeterminada de la aplicacion - español-ARGENTINA -->
		<globalization culture="es-AR" uiCulture="es-AR" />
	</system.web>

	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true"/>
	</system.webServer>
</configuration>
